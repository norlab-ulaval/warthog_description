<?xml version="1.0"?>
<launch>
  <arg name="config" default="base" />

  <set_parameter name="robot_description"
         command="$(find-pkg-share warthog_description)/scripts/env_run
                  $(find-pkg-share warthog_description)/urdf/configs/$(arg config)
                  $(find-pkg-share xacro)/xacro
                  $(find-pkg-share warthog_description)/urdf/warthog.urdf.xacro
                  --inorder" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" exec="robot_state_publisher" />
</launch>
